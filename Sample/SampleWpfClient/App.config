<?xml version="1.0" encoding="utf-8" ?>
<configuration>
	<appSettings>
		<add key="MongoDBName" value="Sample_WpfClient" />
	</appSettings>
	<startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <system.serviceModel>
        <bindings>
            <netMsmqBinding>
                <binding name="NetMsmqBinding_IServiceCommand">
                    <security mode="None" />
                </binding>
            </netMsmqBinding>
            <netTcpBinding>
                <binding name="NetTcpBinding_IServiceMessage">
                    <security mode="None" />
                </binding>
            </netTcpBinding>
        </bindings>
        <client>
            <endpoint address="net.msmq://localhost/private/SampleServiceCommand"
                binding="netMsmqBinding" bindingConfiguration="NetMsmqBinding_IServiceCommand"
                contract="CommandReference.IServiceCommand" name="NetMsmqBinding_IServiceCommand" />
            <endpoint address="net.tcp://localhost:9999/SampleServiceMessage/"
                binding="netTcpBinding" bindingConfiguration="NetTcpBinding_IServiceMessage"
                contract="MessageReference.IServiceMessage" name="NetTcpBinding_IServiceMessage" />
        </client>
    </system.serviceModel>
</configuration>